{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "env": {
    "VITE_CIRCLE_API_TOKEN": "@vite_circle_api_token",
    "VITE_CIRCLE_COMMUNITY_ID": "@vite_circle_community_id"
  }
}


